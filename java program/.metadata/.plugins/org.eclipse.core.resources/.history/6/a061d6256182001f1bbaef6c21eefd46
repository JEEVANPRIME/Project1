package com.CombineTopic;

class Listing{
	Object[] arr;
	int size;
	public Listing() {
		arr=new Object[10];
		size=0;
	}
	
	void resize() {
		int x=size*2;
		Object newArr=new Object[x];
		System.arraycopy(arr, 0, newArr, 0, arr.length);
		arr=newArr;
	}
	
	
}

public class ShoesStore {

	public static void main(String[] args) {
		
	}
}
