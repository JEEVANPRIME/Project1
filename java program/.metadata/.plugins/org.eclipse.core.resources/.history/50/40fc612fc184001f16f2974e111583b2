package com.CombineTopic;

class Emp {
	int id;
	String name;
	double sal;

	public Emp(int id, String name, double sal) {
		this.id = id;
		this.name = name;
		this.sal = sal;
	}

	@Override
	public String toString() {
		return "Emp [id=" + id + ", name=" + name + ", sal=" + sal + "]";
	}
	
	

}

public class MainClassEmp {

	public static void main(String[] args) {

	}
}
